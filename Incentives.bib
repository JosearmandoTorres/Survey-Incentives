  file = {/Users/jtorres0/Zotero/storage/X2SZQDBH/Hsu et al. - 2017 - The Effect of Large Monetary Incentives on Survey Completion Evidence from a Randomized Experiment.pdf;/Users/jtorres0/Zotero/storage/G2NFXIVZ/nfx006.html}
}

@article{Hubbard.Little1988,
