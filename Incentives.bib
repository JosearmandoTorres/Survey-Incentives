@article{GlidewellThomasMacLennanBonettiJohnstonEcclesEdlinPittsClarksonSteenGrimshaw2012,
  title = {Do Incentives, Reminders or Reduced Burden Improve Healthcare Professional Response Rates in Postal Questionnaires? Two Randomised Controlled Trials},
  shorttitle = {Do Incentives, Reminders or Reduced Burden Improve Healthcare Professional Response Rates in Postal Questionnaires?},
  author = {Glidewell, Liz and Thomas, Ruth and MacLennan, Graeme and Bonetti, Debbie and Johnston, Marie and Eccles, Martin P. and Edlin, Richard and Pitts, Nigel B. and Clarkson, Jan and Steen, Nick and Grimshaw, Jeremy M.},
