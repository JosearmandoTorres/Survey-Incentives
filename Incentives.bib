  author = {Wiant, Kristine and Geisen, Emily and Creel, Darryl and Willis, Gordon and Freedman, Andrew and family=Moor, given=Janet, prefix=de, useprefix=true and Klabunde, Carrie},
  date = {2018-10-11},
  journaltitle = {BMC medical research methodology},
