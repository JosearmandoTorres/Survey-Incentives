  shorttitle = {Timing Payments to Subjects of Mail Surveys},
  author = {Schweitzer, Maurice and Asch, David A.},
  date = {1995-11-01},
  journaltitle = {Journal of Clinical Epidemiology},
  shortjournal = {Journal of Clinical Epidemiology},
  volume = {48},
