@article{Asch.etal1998,
  title = {Conducting {{Physician Mail Surveys}} on a {{Limited Budget}}: {{A Randomized Trial Comparing}} \$2 {{Bill}} versus \$5 {{Bill Incentives}}},
  shorttitle = {Conducting {{Physician Mail Surveys}} on a {{Limited Budget}}},
