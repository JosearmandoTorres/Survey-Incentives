}

@article{beckerArePrepaidMonetary2018,
  title = {Are {{Prepaid Monetary Incentives Sufficient}} for {{Reducing Panel Attrition}} and {{Optimizing}} the {{Response Rate}}? {{An Experiment}} in the {{Context}} of a {{Multi-Wave Panel}} with a {{Sequential Mixed-Mode Design}}},
