  keywords = {Cost-Benefit Analysis,Electronic Mail,Female,Health Care Surveys,History 21st Century,Humans,Male,Prospective Studies,Quality Control,Surgeons,Surgery Plastic,Surveys and Questionnaires,United States}
}

@article{Ritter.etal2005,
  title = {Incentive Effects on Cooperation Rates and Sample Composition in the Alcohol and Drug Services Study},
