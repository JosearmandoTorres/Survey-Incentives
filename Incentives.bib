@article{Ulrich.etal2005,
  title = {Does It Pay to Pay? {{A}} Randomized Trial of Prepaid Financial Incentives and Lottery Incentives in Surveys of Nonphysician Healthcare Professionals},
  shorttitle = {Does It Pay to Pay?},
  author = {Ulrich, Connie M. and Danis, Marion and Koziol, Deloris and Garrett-Mayer, Elizabeth and Hubbard, Ryan and Grady, Christine},
