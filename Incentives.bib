}

@article{beckerCashVsVouchers2019,
  title = {Cash vs. Vouchers vs. Gifts in Web Surveys of a Mature Panel Study––{{Main}} Effects in a Long-Term Incentives Experiment across Three Panel Waves},
  author = {Becker, Rolf and Möser, Sara and Glauser, David},
  date = {2019-07-01},
  journaltitle = {Social Science Research},
  shortjournal = {Social Science Research},
  volume = {81},
  pages = {221--234},
