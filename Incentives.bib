@article{ReinischYuLi2016,
  title = {Getting a {{Valid Survey Response From}} 662 {{Plastic Surgeons}} in the 21st {{Century}}},
  author = {Reinisch, John F. and Yu, Daniel C. and Li, Wai-Yee},
