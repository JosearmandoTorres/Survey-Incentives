@article{beckerResearcherIncentivePanelists2023,
  title = {The {{Researcher}}, the {{Incentive}}, the {{Panelists}} and {{Their Response}}: {{The Role}} of {{Strong Reciprocity}} for the {{Panelists}}’ {{Survey Participation}}},
  shorttitle = {The {{Researcher}}, the {{Incentive}}, the {{Panelists}} and {{Their Response}}},
  author = {Becker, Rolf},
  date = {2023-10-13},
  journaltitle = {Survey Research Methods},
  volume = {17},
